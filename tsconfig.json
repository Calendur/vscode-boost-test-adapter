{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["ES2017"],
    "module": "commonjs",
    "outDir": "lib",
    "importHelpers": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "skipLibCheck": true
  },
  "include": [
    "declarations.d.ts",
    "src/main.ts"
  ]
}
